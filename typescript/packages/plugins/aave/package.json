  },
  "peerDependencies": {
    "@goat-sdk/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "zod": "^3.24.1"
  },
  "pnpm": {
